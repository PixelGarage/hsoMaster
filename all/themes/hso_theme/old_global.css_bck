/**********************
 *  Main menu styling
 **********************/

#block-menu-block-1 {
    position: relative;
    z-index: 10;
}

/* position all list items horizontally (horizontal menu, columns)*/
#block-menu-block-1 ul li {
    position: relative;
	float: left;
	width: 145px; /* small column width */
}

/* style all links and nolinks (Group titles) */
#block-menu-block-1 ul li a {
	font-family: 'Soho Gothic W01 Regular';
	display: block;
	color: #fff;
	text-decoration: none;
	font-size: 13px;
	line-height: 40px;
	height: 40px;
	padding: 0 20px;
	background: url(../images/menu_arrow.png) no-repeat 113px 19px;
	position: relative;
}

#block-menu-block-1 ul li.nolink-main,
#block-menu-block-1 ul li .nolink,
#block-menu-block-1 ul li.nolink {
    color: #fff;
    font-size: 15px;
    font-family: 'Soho Gothic W01 Bold';
    text-decoration: none;
    margin-top: 10px;
    padding: 0 10px;
    display: inline-block;
}

#block-menu-block-1 ul li .nolink,
#block-menu-block-1 ul li.nolink {
    font-size: 14px;
    font-family: 'Soho Gothic W01 Medium';
    margin-bottom: 7px;
    padding: 0 15px;
}


/* top menu drop-down arrow styling (only visible when menu is hovered) */
#block-menu-block-1 ul li a span.arrow {
	display: none;
	position: absolute;
	top: 40px;
	left: 105px;
	z-index: 5;
	border-left: 13px solid transparent;
	border-right: 13px solid transparent;
	border-top-width: 13px;
	border-top-style: solid;
}

#block-menu-block-1 ul li.touch_hover span.arrow,
#block-menu-block-1 ul li:hover span.arrow,
#block-menu-block-1 ul li a.active-trail span.arrow {
	display: block;
}

/* KV Kollege top-menu background (normal, hovered) */
#block-menu-block-1 ul li.menu-mlid-986 a { background-color: #94ba40; }
#block-menu-block-1 ul li.menu-mlid-986 a span.arrow { border-top-color: #94ba40; }

#block-menu-block-1 ul li.menu-mlid-986 a.active-trail,
#block-menu-block-1 ul li.menu-mlid-986.touch_hover a,
#block-menu-block-1 ul li.menu-mlid-986:hover a { background-color: #89ab3b; }

#block-menu-block-1 ul li.menu-mlid-986 a.active-trail span.arrow,
#block-menu-block-1 ul li.menu-mlid-986.touch_hover span.arrow,
#block-menu-block-1 ul li.menu-mlid-986:hover span.arrow { border-top-color: #89ab3b; }

/* Handelsschule top-menu background */
#block-menu-block-1 ul li.menu-mlid-987 a { background-color: #e77431; }
#block-menu-block-1 ul li.menu-mlid-987 a span.arrow { border-top-color: #e77431; }

#block-menu-block-1 ul li.menu-mlid-987 a.active-trail,
#block-menu-block-1 ul li.menu-mlid-987.touch_hover a,
#block-menu-block-1 ul li.menu-mlid-987:hover a { background-color: #d76b2d; }

#block-menu-block-1 ul li.menu-mlid-987 a.active-trail span.arrow,
#block-menu-block-1 ul li.menu-mlid-987.touch_hover span.arrow,
#block-menu-block-1 ul li.menu-mlid-987:hover span.arrow { border-top-color: #d76b2d; }

/* Kaderschule top-menu background */
#block-menu-block-1 ul li.menu-mlid-988 a { background-color: #78729e; }
#block-menu-block-1 ul li.menu-mlid-988 a span.arrow { border-top-color: #78729e; }

#block-menu-block-1 ul li.menu-mlid-988 a.active-trail,
#block-menu-block-1 ul li.menu-mlid-988.touch_hover a,
#block-menu-block-1 ul li.menu-mlid-988:hover a { background-color: #706993; }
#block-menu-block-1 ul li.menu-mlid-988 a.active-trail span.arrow,
#block-menu-block-1 ul li.menu-mlid-988.touch_hover span.arrow,
#block-menu-block-1 ul li.menu-mlid-988:hover span.arrow { border-top-color: #706993; }

/* Executive top-menu background */
#block-menu-block-1 ul li.menu-mlid-989 a { background-color: #b1a896; }
#block-menu-block-1 ul li.menu-mlid-989 a span.arrow { border-top-color: #b1a896; }

#block-menu-block-1 ul li.menu-mlid-989 a.active-trail,
#block-menu-block-1 ul li.menu-mlid-989.touch_hover a,
#block-menu-block-1 ul li.menu-mlid-989:hover a { background-color: #a39b88; }
#block-menu-block-1 ul li.menu-mlid-989 a.active-trail span.arrow,
#block-menu-block-1 ul li.menu-mlid-989.touch_hover span.arrow,
#block-menu-block-1 ul li.menu-mlid-989:hover span.arrow { border-top-color: #a39b88; }

/*
 *  Menu block (dropdown) styling
 */

/* menu block (invisible and animated when hovered) */
#block-menu-block-1 ul li ul.menu {
    display: none;
    position: absolute;
	left: 0;
	top: 40px;
	width: 470px;   /* 2 normal columns wide */
	z-index: 30;
	padding: 20px 0;
}

/* last two columns in menu block (Informationen, Ausbildungskonzept) */
#block-menu-block-1 ul li ul.menu ul.menu {
    display: block;
    position: static;
	width: 235px;   /* normal column width */
	padding: 0;
}

/* menu block styling and positioning  (small device menu) */
#block-menu-block-1 ul li.menu-mlid-986 ul.menu {
    background: #95ba40;    /* position left = 0 (below first top menu) */
}
#block-menu-block-1 ul li.menu-mlid-987 ul.menu {
    background: #e77431;    /* position left = 145 (below second top menu) */
}
#block-menu-block-1 ul li.menu-mlid-988 ul.menu {
    background: #78729e;
    left: -290px;   /* 2 small columns left-shift (starts below first top menu)  */
}
#block-menu-block-1 ul li.menu-mlid-988 ul.menu.double {
    left: -290px;   /* 2 small columns left-shift (starts below first top menu)  */
}
#block-menu-block-1 ul li.menu-mlid-989 ul.menu {
    background: #b1a896;
    left: -325px;   /* left-shift, so that menu ends with last top menu  */
}
#block-menu-block-1 ul li.menu-mlid-989 ul.menu.double {
    left: -325px;   /* left-shift, so that menu ends with last top menu  */
}

/* menu block columns with right border */
#block-menu-block-1 ul li ul.menu li {
    width: 234px;   /* one normal column width (without right border) */
	border-right: 1px solid #86ab3b;
}
#block-menu-block-1 ul li.menu-mlid-987 ul.menu li {
	border-right: 1px solid #d76b2d;
}
#block-menu-block-1 ul li.menu-mlid-988 li.double .view-content,
#block-menu-block-1 ul li.menu-mlid-988 ul.menu li {
	border-right: 1px solid #706993;
}
#block-menu-block-1  ul li.menu-mlid-989 li.double .view-content,
#block-menu-block-1 ul li.menu-mlid-989 ul.menu li {
	border-right: 1px solid #a39b88;
}
#block-menu-block-1 ul li ul.menu li.last,
#block-menu-block-1 ul li ul.menu li li {
	border-right: none !important;
}

/* do not display first menu column (output from view) in small/medium size menu */
#block-menu-block-1 ul li ul.menu li.menu-views {
    display: none;
}

/* define width of sub-columns when doubled */
#block-menu-block-1 li.double .view-content,
#block-menu-block-1 li.double .view-content-right {
    float: left;
    width: 234px;
}
#block-menu-block-1 li.double .view-content-right {
    padding-top: 33px;  /* aligns top of sub-columns */
}

/* menu rows inside columns */
#block-menu-block-1 ul li ul.menu li li {
	width: 100%;
}

/* menu hyper-links styling */
#block-menu-block-1 ul li.touch_hover ul.menu li a,
#block-menu-block-1 ul li:hover ul.menu li a,
#block-menu-block-1 ul li ul.menu li a {
    background: transparent;
    font-size: 12px;
    line-height: 13px;
    min-height: 20px;
    padding: 0 5px 7px 20px;
}
#block-menu-block-1 ul li ul.menu li a {
    height: auto;
}

#block-menu-block-1 ul li ul.menu li a:hover {
	color: #5d7820;
}
#block-menu-block-1 ul li.menu-mlid-987 ul.menu li a:hover {
	color: #99400d;
}
#block-menu-block-1 ul li.menu-mlid-988 ul.menu li a:hover {
	color: #454166;
}
#block-menu-block-1 ul li.menu-mlid-989 ul.menu li a:hover {
	color: #efefef;
}



